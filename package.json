{
  "name": "u11-fullstack-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@svgr/webpack": "^5.5.0",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "next": "^10.0.5",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.6.2",
    "next-react-svg": "^1.1.2",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.14.2",
    "react-router-dom": "^5.2.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "prettier",
      "prettier/react"
    ],
    "globals": {
      "localStorage": true,
      "fetch": true
    },
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "semi": 1,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto",
          "semi": true
        }
      ]
    }
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  }
}
